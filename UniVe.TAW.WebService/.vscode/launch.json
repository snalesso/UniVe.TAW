    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch WebService",
            "program": "${workspaceFolder}/src/application/Bootstrapper.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "preLaunchTask": "Transpile",
            "envFile": "${workspaceFolder}/.env",
            "internalConsoleOptions": "openOnSessionStart",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Export dependencies & launch WebService",
            "program": "${workspaceFolder}/src/application/Bootstrapper.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "preLaunchTask": "Export dependencies",
            "envFile": "${workspaceFolder}/.env",
            "internalConsoleOptions": "openOnSessionStart",
        },
    ]
}