{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.*proj.user": true,
        "**/*.*proj": true,
        "**/*.sln": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.js.map": {
            "when": "$(basename)"
        },
        "**/package-lock.json": true,
        "**/.sourcemaps": true
    }
}